{
    "compilerOptions": {
        "baseUrl": "./",
        "target": "ES2022",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "CommonJS",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "outDir": "./dist"
    },
    "exclude": [
        "node_modules",
    ]
}